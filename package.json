{
  "name": "github-starboard",
  "private": true,
  "version": "31.0.0",
  "author": "Daiwei Lu <daiweilu123@gmail.com> (http://daiwei.lu)",
  "license": "MIT",
  "engines": {
    "node": "5.10.0",
    "npm": "3.8.3"
  },
  "dependencies": {
    "axios": "0.9.1",
    "classnames": "2.2.3",
    "mixpanel-browser": "^2.7.2",
    "nprogress": "0.2.0",
    "observe-object-path": "^0.3.0",
    "ramda": "0.21.0",
    "react": "0.14.8",
    "react-dnd": "2.1.2",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "0.14.8",
    "react-modal": "0.6.1",
    "react-switch-path": "0.0.2",
    "redux": "3.3.1",
    "redux-promise-middleware": "^2.3.3",
    "redux-thunk": "2.0.1",
    "reselect": "2.3.0",
    "routility": "^0.1.1",
    "socket.io-client": "1.4.5",
    "svg-react-loader": "0.3.3",
    "updeep": "0.13.0",
    "validate.js": "0.9.0"
  },
  "devDependencies": {
    "babel-core": "6.7.5",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "browser-sync": "2.11.2",
    "convict": "^1.3.0",
    "eslint": "2.7.0",
    "eslint-plugin-node": "^1.0.0",
    "eslint-plugin-react": "4.3.0",
    "file-loader": "0.8.5",
    "node-sass": "3.4.2",
    "onchange": "^2.4.0",
    "redux-logger": "2.6.1",
    "webpack": "1.12.14"
  },
  "scripts": {
    "clean": "rm -rf public",
    "lint": "eslint --rulesdir src *.js",
    "watch:lint": "npm run lint -s & onchange 'src/**/*.js' '*.js' -v -- npm run lint -s",
    "test": "",
    "check": "npm run lint && npm test",
    "build:client:js": "webpack",
    "build:client:css": "node-sass --output-style expanded -o public client/style/index.scss",
    "build": "npm run build:client:js && npm run build:client:css",
    "watch:build:client:js": "npm run build:client:js -- -w",
    "watch:build:client:css": "npm run build:client:css -- -w",
    "watch:build": "npm run watch:build:client:js & npm run watch:build:client:css",
    "dev": "browser-sync start --server public --files='public/**' --port 10010"
  }
}
